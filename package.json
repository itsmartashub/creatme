{
  "name": "creatme",
  "author": "Marta Bires",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "dev": "parcel src/index.html --port 1234 --no-source-maps --no-cache",
    "build:parcel": "parcel build src/index.html --no-source-maps --no-cache",
    "build": "npm run clean && npm run build:parcel",
    "clean": "rm -rf dist/*"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.6.2",
    "@parcel/transformer-sass": "2.6.2",
    "@parcel/transformer-xml": "^2.6.2",
    "autoprefixer": "^10.4.7",
    "parcel": "^2.6.2",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "postcss": "^8.1.0"
  }
}
